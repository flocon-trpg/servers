        "@flocon-trpg/tsconfig": "workspace:^",
        "@typescript-eslint/eslint-plugin": "5.62.0",
        "@typescript-eslint/parser": "5.62.0",
        "eslint": "8.57.1",
        "eslint-config-prettier": "9.1.0",
        "eslint-import-resolver-typescript": "3.6.3",
        "eslint-plugin-import": "2.30.0",
        "prettier": "3.3.3",
        "rimraf": "5.0.10",
