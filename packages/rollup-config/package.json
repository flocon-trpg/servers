        "@flocon-trpg/tsconfig": "workspace:^",
        "eslint": "9.13.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-import": "2.31.0",
        "prettier": "3.3.3",
        "typescript": "5.6.2",
        "typescript-eslint": "8.11.0"
    },
    "peerDependencies": {
        "rollup": "^4.0.0"
    }
