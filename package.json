{
    "version": "0.15.5",
    "license": "MIT",
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "typings": "dist/types/index.d.ts",
    "files": [
        "dist"
    ],
    "engines": {
        "node": ">=14"
    },
    "scripts": {
        "build": "yarn lint && yarn prettier && rimraf ./dist && tsc -p tsconfig.build-cjs.json && tsc -p tsconfig.build-esm.json && tsc -p tsconfig.build-types.json",
        "test": "jest",
        "prepare": "yarn build && husky install",
        "lint": "eslint src test",
        "prettier": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \".github/**/*.yml\""
    },
    "prettier": {
        "printWidth": 100,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "es5",
        "tabWidth": 4,
        "endOfLine": "crlf",
        "arrowParens": "avoid"
    },
    "name": "@kizahasi/flocon-core",
    "author": "kizahasi",
    "description": "Core library for Flocon project(not released yet).",
    "repository": {
        "type": "git",
        "url": "https://github.com/kizahasi/flocon-core"
    },
    "devDependencies": {
        "@types/jest": "27.0.1",
        "@types/lodash.clonedeep": "4.5.6",
        "@typescript-eslint/eslint-plugin": "4.29.1",
        "@typescript-eslint/parser": "4.29.1",
        "eslint": "7.32.0",
        "eslint-config-prettier": "8.3.0",
        "fast-check": "2.17.0",
        "fast-check-io-ts": "0.5.0",
        "husky": "7.0.1",
        "jest": "27.0.6",
        "prettier": "2.3.2",
        "rimraf": "3.0.2",
        "ts-jest": "27.0.4",
        "typescript": "4.3.5"
    },
    "dependencies": {
        "@kizahasi/flocon-script": "^0.6.0",
        "@kizahasi/ot-string": "^0.2.3",
        "@kizahasi/result": "^1.0.0",
        "@kizahasi/util": "^0.8.0",
        "@ltd/j-toml": "^1.12.2",
        "fp-ts": "^2.10.5",
        "io-ts": "^2.2.16",
        "lodash.clonedeep": "^4.5.0"
    }
}
