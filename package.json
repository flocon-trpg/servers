{
    "version": "0.4.0",
    "license": "MIT",
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "typings": "dist/types/index.d.ts",
    "files": [
        "dist"
    ],
    "engines": {
        "node": ">=12"
    },
    "scripts": {
        "build": "yarn lint && yarn prettier && rimraf ./dist && tsc -p tsconfig.build-cjs.json && tsc -p tsconfig.build-esm.json && tsc -p tsconfig.build-types.json",
        "test": "jest",
        "prepare": "yarn build",
        "lint": "eslint src test",
        "prettier": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\""
    },
    "prettier": {
        "printWidth": 100,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "es5",
        "tabWidth": 4,
        "endOfLine": "crlf",
        "arrowParens": "avoid"
    },
    "name": "@kizahasi/flocon-script",
    "author": "kizahasi",
    "repository": {
        "type": "git",
        "url": "https://github.com/kizahasi/flocon-script"
    },
    "devDependencies": {
        "@types/estree": "^0.0.49",
        "@types/jest": "^26.0.24",
        "@typescript-eslint/eslint-plugin": "^4.28.2",
        "@typescript-eslint/parser": "^4.28.2",
        "eslint": "^7.30.0",
        "eslint-config-prettier": "^8.3.0",
        "jest": "^27.0.6",
        "prettier": "^2.3.2",
        "rimraf": "^3.0.2",
        "ts-jest": "^27.0.3",
        "typescript": "^4.3.5"
    },
    "dependencies": {
        "@kizahasi/option": "^0.1.3",
        "@kizahasi/util": "^0.2.3",
        "acorn": "^8.4.1"
    }
}
