{
    "dependencies": {
        "@graphql-codegen/cli": "^1.21.3",
        "@graphql-codegen/typescript": "^1.21.1",
        "@graphql-codegen/typescript-operations": "^1.17.15",
        "@graphql-codegen/typescript-react-apollo": "^2.2.3",
        "@mikro-orm/core": "^4.5.3",
        "@mikro-orm/migrations": "^4.5.3",
        "@mikro-orm/postgresql": "^4.5.3",
        "@mikro-orm/sqlite": "^4.5.3",
        "@types/color": "^3.0.1",
        "@types/diff-match-patch": "^1.0.32",
        "@types/node": "^12",
        "@types/uuid": "^8.3.0",
        "@types/yargs": "^16.0.1",
        "apollo-server-express": "^2.23.0",
        "bcdice": "^3.1.2",
        "class-transformer": "^0.4.0",
        "class-validator": "^0.13.1",
        "color": "^3.1.3",
        "diff-match-patch": "^1.0.5",
        "firebase-admin": "^9.6.0",
        "graphql": "^15.5.0",
        "graphql-ws": "^4.4.1",
        "immer": "^9.0.1",
        "node-cache": "^5.1.2",
        "nodemon": "^2.0.7",
        "npm-run-all": "^4.1.5",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^6.6.7",
        "ts-node": "^9.1.1",
        "type-graphql": "^1.1.1",
        "typescript": "^4.2.4",
        "yargs": "^16.2.0"
    },
    "devDependencies": {
        "@types/jest": "^26.0.22",
        "@typescript-eslint/eslint-plugin": "^4.22.0",
        "@typescript-eslint/parser": "^4.22.0",
        "eslint": "^7.24.0",
        "eslint-config-prettier": "^8.2.0",
        "eslint-plugin-prettier": "^3.4.0",
        "husky": "^6.0.0",
        "jest": "^26.6.3",
        "prettier": "^2.2.1",
        "rimraf": "^3.0.2",
        "ts-jest": "^26.5.5"
    },
    "engines": {
        "node": "12.x"
    },
    "scripts": {
        "dev": "node dist/index.js",
        "build": "tsc",
        "build-release": "rimraf ./dist && tsc",
        "gen": "run-s gen:schema gen:codegen",
        "gen:codegen": "graphql-codegen --config codegen.yml",
        "gen:schema": "ts-node run-buildSchemaSync.ts",
        "start": "node dist/index.js",
        "migration-check": "ts-node run-migrationCheck.ts",
        "migration-create": "ts-node run-migrationCreate.ts",
        "migration-up": "ts-node run-migrationUp.ts",
        "migration-down": "ts-node run-migrationDown.ts",
        "test": "jest",
        "watch": "run-p watch:*",
        "watch:codegen": "graphql-codegen --config codegen.yml --watch",
        "watch:schema": "nodemon --config nodemon.buildSchema.json",
        "watch:node": "nodemon --config nodemon.node.json",
        "prepare": "husky install"
    },
    "license": "MIT"
}
