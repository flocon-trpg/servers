{
    "dependencies": {
        "@graphql-codegen/cli": "^1.19.2",
        "@graphql-codegen/typescript": "^1.17.11",
        "@graphql-codegen/typescript-operations": "^1.17.10",
        "@graphql-codegen/typescript-react-apollo": "^2.1.1",
        "@mikro-orm/core": "^4.3.2",
        "@mikro-orm/migrations": "^4.3.2",
        "@mikro-orm/postgresql": "^4.3.2",
        "@mikro-orm/sqlite": "^4.3.2",
        "@types/color": "^3.0.1",
        "@types/diff-match-patch": "^1.0.32",
        "@types/faker": "^5.1.4",
        "@types/node": "^12",
        "@types/uuid": "^8.3.0",
        "@types/yargs": "^16.0.0",
        "apollo-server-express": "^2.19.0",
        "bcdice": "^1.6.0",
        "class-transformer": "^0.3.1",
        "class-validator": "^0.12.2",
        "color": "^3.1.3",
        "diff-match-patch": "^1.0.5",
        "firebase-admin": "^9.4.1",
        "graphql": "^15.4.0",
        "immer": "^8.0.0",
        "nodemon": "^2.0.6",
        "npm-run-all": "^4.1.5",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^6.6.3",
        "ts-node": "^9.0.0",
        "type-graphql": "^1.1.1",
        "typescript": "^4.1.3",
        "yargs": "^16.2.0"
    },
    "devDependencies": {
        "@types/jest": "^26.0.15",
        "@typescript-eslint/eslint-plugin": "^4.8.1",
        "@typescript-eslint/parser": "^4.8.1",
        "eslint": "^7.13.0",
        "eslint-config-prettier": "^6.15.0",
        "eslint-plugin-prettier": "^3.1.4",
        "jest": "^26.6.3",
        "prettier": "^2.1.2",
        "ts-jest": "^26.4.4"
    },
    "engines": {
        "node": "12.x"
    },
    "scripts": {
        "dev": "node dist/index.js",
        "build": "tsc",
        "gen": "run-s gen:schema gen:codegen",
        "gen:codegen": "graphql-codegen --config codegen.yml",
        "gen:schema": "ts-node run-buildSchemaSync.ts",
        "start": "node dist/index.js",
        "migration-create": "ts-node run-migrationCreate.ts",
        "migration-up": "ts-node run-migrationUp.ts",
        "test": "jest",
        "watch": "run-p watch:*",
        "watch:codegen": "graphql-codegen --config codegen.yml --watch",
        "watch:schema": "nodemon --config nodemon.buildSchema.json",
        "watch:node": "nodemon --config nodemon.node.json"
    },
    "license": "MIT"
}
