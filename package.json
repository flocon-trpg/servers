        "@kizahasi/result": "^1.0.0",
        "@kizahasi/util": "^0.8.0",
        "@ltd/j-toml": "^1.12.2",
        "fp-ts": "^2.10.5",
        "io-ts": "^2.2.16",
        "ioredis": "^4.27.8",
        "lodash.clonedeep": "^4.5.0",
        "node-cache": "^5.1.2"
