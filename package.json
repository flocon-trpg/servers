{
    "version": "0.13.2",
    "license": "MIT",
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "typings": "dist/types/index.d.ts",
    "files": [
        "dist"
    ],
    "engines": {
        "node": ">=14"
    },
    "scripts": {
        "build": "yarn lint && yarn prettier && rimraf ./dist && tsc -p tsconfig.build-cjs.json && tsc -p tsconfig.build-esm.json && tsc -p tsconfig.build-types.json",
        "test": "jest",
        "prepare": "yarn build",
        "lint": "eslint src test",
        "prettier": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\""
    },
    "prettier": {
        "printWidth": 100,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "es5",
        "tabWidth": 4,
        "endOfLine": "crlf",
        "arrowParens": "avoid"
    },
    "name": "@kizahasi/flocon-core",
    "author": "kizahasi",
    "description": "Core library for Flocon project(not released yet).",
    "repository": {
        "type": "git",
        "url": "https://github.com/kizahasi/flocon-core"
    },
    "devDependencies": {
        "@types/jest": "^26.0.24",
        "@types/lodash.clonedeep": "^4.5.6",
        "@typescript-eslint/eslint-plugin": "^4.28.2",
        "@typescript-eslint/parser": "^4.28.2",
        "eslint": "^7.30.0",
        "eslint-config-prettier": "^8.3.0",
        "jest": "^27.0.6",
        "prettier": "^2.3.2",
        "rimraf": "^3.0.2",
        "ts-jest": "^27.0.3",
        "typescript": "^4.3.5"
    },
    "dependencies": {
        "@kizahasi/flocon-script": "^0.5.3",
        "@kizahasi/ot-string": "^0.2.2",
        "@kizahasi/result": "^0.3.0",
        "@kizahasi/util": "^0.2.3",
        "@ltd/j-toml": "^1.12.2",
        "fp-ts": "^2.10.5",
        "io-ts": "^2.2.16",
        "lodash.clonedeep": "^4.5.0"
    }
}
