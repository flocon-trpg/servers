    "scripts": {
        "dev": "run-s build:less dev:next",
        "dev:next": "next",
        "build": "run-s build:deps build:self",
        "build:deps": "yarn workspaces foreach -pvtR --from '@flocon-trpg/web-server' --exclude '@flocon-trpg/web-server' run build",
        "build:self": "run-s build:less build:next",
        "build:less": "lessc ./src/styles/css/antd.less ./src/styles/css/antd.css --js",
        "build:next": "next build",
        "export": "run-s build export:core",
        "export:core": "next export",
        "start": "next start",
        "serve": "serve ./out",
        "test": "jest",
        "type-check": "tsc",
        "tc": "yarn run type-check",
        "storybook": "run-s build:less storybook:core",
        "storybook:core": "start-storybook -p 6006",
        "build-storybook": "yarn build:less && yarn build-storybook:core",
        "build-storybook:core": "build-storybook"
